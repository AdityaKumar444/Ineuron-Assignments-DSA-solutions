// SLL.cpp is provided in folder

#include<bits/stdc++.h>
#include"SLL.cpp"
using namespace std ;
class STACK : public SLL 
{
    public:
        STACK() : SLL() {}
        void push(int);
        void pop();
};
void STACK::push(int item)
{
    insertAtLast(item);
}
void STACK::pop()
{
    deleteLast();
}
